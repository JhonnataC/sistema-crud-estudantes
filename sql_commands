CREATE TABLE IF NOT EXISTS students (
	id SERIAL PRIMARY KEY,
	age INTEGER NOT NULL,
	code INTEGER NOT NULL UNIQUE,
	name TEXT NOT NULL
)


INSERT INTO students(code, name, age) VALUES (10001111, 'Chris', 16)
